<%- include('../partials/head.ejs')%>
<%- include('../partials/navadmin.ejs') %>
<div class="container">
  <form class="form-group p-5 bg-light" method="post" action="/admin/updated"><br>
    <label for="titulo">Nome do produto: </label>
    <input type="text" name="titulo" value=""><br><br>
    <label for="preco">Valor: </label>
    <input type="number" step="0.01" name="preco" value=""><br><br>
    <label for="imagem">URL: </label>
    <input type="text" name="imagem" value=""><br><br>
    <label for="status">Status:</label>
    <div class="form-check">
      <input class="form-check-input" type="radio" name="status" value="true">
      <label class="form-check-label" for="status">
        Ativo
      </label>
    </div>
    <div class="form-check">
      <input class="form-check-input" type="radio" name="status" value="false" checked>
      <label class="form-check-label" for="status">
        Inativo
      </label>
    </div><br>
    <button class="btn btn-primary" type="submit">Atualizar</button>
  </form>
  <br>
  <a href="/" class=" px-5"><button class="btn btn-primary">voltar para Inicio</button></a>
</div>

<%- include('../partials/foot.ejs')%>
